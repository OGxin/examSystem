
page{
     height: 100%;
      background: #91e7f6;  
 }
.page{
    font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
    height: 1000rpx;
    background: #ffffff;
    margin: 15rpx 15rpx 70rpx 15rpx;
     position: relative; 
      z-index: 2;
}

.bottimBG{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 150rpx;
   z-index: 1;
 
}

.row{
  display:flex;
   align-items:center; 
  justify-content:space-around;
}

.item{
   width: 25px;
  height: 25px;
  background-color: #9ad4fc;
  color: white;
  text-align: center;
  border-radius: 100px;
  border:1px #9ad4fc solid;
  justify-content:center;
  display:flex;
  align-items:center;
  margin-top: 30rpx;
}

.item2{
   width: 25px;
  height: 25px;
  background-color: #fc9a9a;
  color: white;
  text-align: center;
  border-radius: 100px;
  border:1px #fc9a9a solid;
  justify-content:center;
  display:flex;
  align-items:center;
  margin-top: 30rpx;
}

 .bottom{
  width: 100%;
  position: absolute;
  bottom: 0rpx;
  height: 100rpx;
  border-top: 1px solid #c9c9d8;
  display:flex;
   align-items:center; 
  justify-content:space-around;
} 